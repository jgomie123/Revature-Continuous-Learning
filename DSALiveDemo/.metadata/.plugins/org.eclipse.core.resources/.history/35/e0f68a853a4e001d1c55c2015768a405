package stackfromscratch;

public class StackDriver {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		SimpleStack myStack = new SimpleStack(4);
		myStack.push(20);
		myStack.push(30);
		myStack.push(40);
		myStack.push(50);
		
		while(!myStack.isEmpty()) {
			long value = myStack.pop();
			System.out.println(value);
		}
		
		
		/*
		 * Challenge: 
		 * Create a method that will reverse the order of a string chatracter by character. 
		 * First: create a class that will initialize a stack of chars
		 * 
		 * */
		
		
	}

}
